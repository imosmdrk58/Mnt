{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.ico",
      "dest": "/dist/public/favicon.svg"
    },
    {
      "src": "/favicon\\.svg",
      "dest": "/dist/public/favicon.svg"
    },
    {
      "src": "/api/setup/status",
      "dest": "/api/setup/status.js"
    },
    {
      "src": "/api/setup/install",
      "dest": "/api/setup/install.js"
    },
    {
      "src": "/api/setup/validate-db",
      "dest": "/api/setup/validate-db.js"
    },
    {
      "src": "/api/setup/test-current-db",
      "dest": "/api/setup/test-current-db.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}